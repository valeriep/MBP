.listeActus {
	clear: both;
}

.listeActus h2 {
	font-size: 2.75em;
	color: #ff6600;
	background: url('../images/h1_puce.png') left 6px no-repeat;
	margin-bottom: 0.85em;
	padding-left: 30px;
}

.ficheActus {
	margin-bottom: 28px;
	padding-bottom: 18px;
	border-bottom: 8px dotted #e4e4e4;
}

.ficheActus:after {
	clear: both;
    content: "";
    display: table;
    line-height: 0;
}

.ficheActus:last-child {
	margin-bottom: 30px;
	padding-bottom: 0;
	border-bottom: none;
}

.ficheActus:nth-child(even) {}


.ficheActus img {
	float: left;
	max-width: 48%;
	margin: 0 4% 10px 0;
	padding: 0;
}

.dateActu {
	font-size: 0.95em;
	font-weight: bold;
	color: #ffffff;
	background-color: #73c111;
	padding: 5px 12px 5px 12px;
	margin: 0 0 10px 0;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	
	-webkit-box-shadow: 0 0 5px #999999;
	-moz-box-shadow: 0 0 5px #999999;
	box-shadow: 0 0 5px #999999;
}

.titreActu {
	display: block;
	font-size: 1.7em;
	line-height: 1em;
	font-weight: bold;
	color: #73c111;
	margin-top: 0.75em;
	margin-bottom: 0.5em;
}





@media (max-width: 480px) {
	.cv3-2cols .colg, .cv3-2cols .cold,
	.cv3-3cols .cols1, .cv3-3cols .cols2, .cv3-3cols .cols3 {
		-moz-box-sizing: border-box;
		float: none;
		width: 100%;
		margin-left: 0;
	}
	
	.cv3-imagetxt img, .cv3-txtimage img, .cv3-download img {
		float: none;
		max-width: 100%;
		margin: 0 0 10px 0;
		padding: 0;
	}

	.cv3-1234images .diaporama4 {
		width: 48.7179%;
		margin-left: 2.5641%;
	}
	
	.cv3-1234images [class*="diaporama"]:nth-child(odd) {
		margin-left: 0;
	}

	.cv8d-insert label, .cv8d-comment, .cv8d-answer, .cv8d-action {
		clear: both;
		float: none;
		width: 100%;
		text-align: left;
		margin: 0 0 0.75em 0;
	}
	
}


