}

.cv3-3cols .cols1 {
	margin-left: 0;
}

.cv3-3cols .top {
	display: none;
	height: 0;
}

.cv3-3cols .top img {
	width: inherit;
}

.cv3-3cols .image img {
	-moz-border-radius-topright: 8px;
	-moz-border-radius-topleft: 8px;
	-webkit-border-radius-topright: 8px;
	-webkit-border-radius-topleft: 8px;
	border-top-right-radius: 8px;
	border-top-left-radius: 8px;
}


.cv3-3cols .titre {
	font-size: 1.35em;
	font-weight: bold;
	color: #ffffff;
	background: #73c111;
	padding: 15px 15px 5px 15px;
}

.cv3-3cols .txt {
	color: #ffffff;
	padding: 0 15px 8px 15px;
	background: #73c111;
	-moz-border-radius-bottomright: 8px;
	-moz-border-radius-bottomleft: 8px;
	-webkit-border-radius-bottomright: 8px;
	-webkit-border-radius-bottomleft: 8px;
	border-bottom-right-radius: 8px;
	border-bottom-left-radius: 8px;
}

.cv3-3cols .bas {
	height: 20px;
	border-top: 1px solid #ffffff;
	background: #ffffff url(../images/cv3-3cols_bas_fond.png) center top no-repeat;
}




/* tableau */
.tab_base {
	width: 100%;
	border-collapse: collapse;
	margin: 0 0 0 0;
	padding: 0 0 0 0;
}

.tab_base caption {
	color: #ff6600;
	text-align: left;
	font-weight: bold;
	font-size: 2em;
	padding:  0 0 10px 0;
}

.tab_base td {
	vertical-align: top;
	border-bottom: 1px dotted #ff6600;
	padding: 7px 5px;
}

.tab_base th {
	font-weight: normal;
	color: #ffffff;
	vertical-align: top;
	text-transform: uppercase;
	padding: 6px;
	border-bottom: 1px solid #ff6600;
	background-color: #ff6600;
}


/* Formulaire dynamique */
.cv8d-insert {
	clear: both;
	margin-bottom: 35px;
}
.cv8d-insert input,	
.cv8d-insert label,	
.cv8d-insert img,	
.cv8d-insert select {}

.cv8d-insert legend {}

