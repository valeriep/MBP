	margin: 0;
	padding: 5px 0 10px 0;
}

/* Formulaire > boutton */
.cv8d-action {
	float: left;
	width: 48.7179%;
	/*width: 74.359%; 2/3 */
	height: auto;
	/*margin: 0 0 0.75em 25.641%; 2/3 tier*/
	margin: 0 0 1.25em 25.641%;
}


.tzr-page-content .cv8d-action input {
	cursor:pointer;
	width:auto;
	font-size:1.25em;
	font-family:Arial, helvetica, verdana, sans-serif;
	font-weight:bold;
	color:#ffffff;	
	text-transform:uppercase;
	line-height:18px;
	background-color:#ff6600;
	text-align:center;
	padding:6px 15px 4px 15px;
	border:1px solid #ff6600;

	-moz-border-radius: 8px;
	-webkit-border-radius: 8px;
	border-radius: 8px;

	-webkit-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.55);
	-moz-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.55);
	box-shadow: 0 1px 5px rgba(0, 0, 0, 0.55);
}

.tzr-page-content .cv8d-action a{
	text-decoration:none;
}
.tzr-page-content .cv8d-action input:hover {
	color:#ff6600;
	background-color:#ffffff;
	border:1px solid #ff6600;
	-webkit-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.25);
	-moz-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.25);
	box-shadow: 0 1px 5px rgba(0, 0, 0, 0.25);
}

.tzr-page-content .cv8d-action input:active {
 	color:#ffffff;
	background-color:#73c111;
	border:1px solid #73c111;
	-webkit-box-shadow: 0 1px 5px rgba(0, 0, 0, 0);
	-moz-box-shadow: 0 1px 5px rgba(0, 0, 0, 0);
	box-shadow: 0 1px 5px rgba(0, 0, 0, 0);
}





