


h1, h2, h3, h4, h5, h6 {
	line-height: inherit;
	line-height: normal;
	margin: 0 0 0 0;
	padding: 0 0 0 0;
}

h1 {
	font-size: 1.8em;
	color: #000000;
	margin-bottom: 0.5em;
}

h2 {
	font-size: 1.4em;
	color: #ea5b0c;
	margin-bottom: 0.5em;
}

h3 {
	font-size: 1.95em;
	color: #ff6600;
	margin-bottom: 0.5em;
}

h4 {
	font-size: 1.95em;
	font-weight: normal;
	color: #73c111;
	margin-bottom: 0.4em;
}

h5 {
	font-size: 1.95em;
	font-weight: normal;
	color: #ff6600;
	margin-bottom: 0.4em;
}

h6 {
	font-size: 1.5em;
	color: #686357;
	margin-bottom: 0.4em;
}

p {
	line-height: 1.5em;
	padding: 0 0 0.6em 0;
	margin: 0;
}

.tzr-page-content ul {
	list-style-image: url(../images/ul_puce1.png);
	list-style-type: square ;
	list-style-position: outside;
	line-height: 1.5em;
	margin: 0 0 15px 15px;
	padding: 0;
}

.tzr-page-content ul li {
	padding: 0 0 5px 0;
}

.tzr-page-content ul ul {
	list-style-image: url(../images/ul_puce2.png);
	margin: 5px 0 0 15px;
	padding: 0;
}


.legende {
	font-size: 1em;
	line-height: 1em;
	font-style: italic;
	color: #ff6600;
}



.cv3-txtimage:after,
.cv3-imagetxt:after,
.cv3-1234images:after,
.cv3-flash:after,
.cv3-download:after,
.cv3-slider:after,
.cv3-video:after,
cv3-audio:after,
.cv3-envaleur:after,
.cv3-2cols:after,
