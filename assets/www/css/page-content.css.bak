	}
	
	.navigation .icon-emploi-left {
		background-position: -160px -401px;
	}
	
	.navigation .icon-entreprise-left {
		background-position: -210px -401px;
	}
	
	.navigation .icon-quotidien-left {
		background-position: -260px -401px;
	}
	
	.navigation .icon-autre-left {
		background-position: -310px -401px;
	}

