	background: url('../images/cv3-chapo_fond.png') right top no-repeat;
}



.cv3-txtimage {
	clear: both;
	margin-bottom: 25px;
}

.cv3-txtimage img {
	float: right;
	max-width: 48%;
	margin: 0 0 10px 4%;
